<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.StoreController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Resources/insideStore.png"/>
            </image>
        </ImageView>
        <Button fx:id="buyFood" layoutX="481.0" layoutY="179.0" mnemonicParsing="false" onMouseClicked="#setBuyFood"
                onMouseEntered="#foodmouseEnter" onMouseExited="#foodmouseLeave" opacity="0.0" prefHeight="74.0"
                prefWidth="94.0" text="Buy Food"/>
        <Button fx:id="buySmithore" layoutX="57.0" layoutY="97.0" mnemonicParsing="false"
                onMouseClicked="#setBuySmithore" onMouseEntered="#oremouseEnter" onMouseExited="#oremouseLeave"
                opacity="0.0" prefHeight="74.0" prefWidth="94.0" text="Buy Smithore"/>
        <Button fx:id="buyEnergy" layoutX="65.0" layoutY="179.0" mnemonicParsing="false" onMouseClicked="#setBuyEnergy"
                onMouseEntered="#energymouseEnter" onMouseExited="#energymouseLeave" opacity="0.0" prefHeight="74.0"
                prefWidth="94.0" text="Buy Energy"/>
        <Button fx:id="buyMule" layoutX="481.0" layoutY="97.0" mnemonicParsing="false" onMouseClicked="#setBuyMule"
                onMouseEntered="#mulemouseEnter" onMouseExited="#mulemouseLeave" opacity="0.0" prefHeight="74.0"
                prefWidth="94.0" text="Buy Mule"/>
        <Button layoutX="69.0" layoutY="17.0" mnemonicParsing="false" onMouseEntered="#cmouseEnter"
                onMouseExited="#cmouseLeave" opacity="0.0" prefHeight="74.0" prefWidth="94.0" text="Buy crystite"/>
        <Button fx:id="leaveStore" layoutX="14.0" layoutY="360.0" mnemonicParsing="false"
                onMouseClicked="#setLeaveStore" text="Leave Store"/>
        <ImageView fitHeight="152.0" fitWidth="121.0" layoutX="255.0" layoutY="36.0">
            <image>
                <Image url="@Resources/store-man.gif"/>
            </image>
        </ImageView>
        <ImageView fx:id="foodAnim" fitHeight="52.0" fitWidth="49.0" layoutX="500.0" layoutY="180.0" visible="false">
            <image>
                <Image url="@Resources/food-anim.gif"/>
            </image>
        </ImageView>
        <ImageView fx:id="cAnim" fitHeight="96.0" fitWidth="84.0" layoutX="81.0" layoutY="6.0" visible="false">
            <image>
                <Image url="@Resources/crystite-anim.gif"/>
            </image>
        </ImageView>
        <ImageView fx:id="oreAnim" fitHeight="73.0" fitWidth="83.0" layoutX="55.0" layoutY="99.0" visible="false">
            <image>
                <Image url="@Resources/ore-anim.gif"/>
            </image>
        </ImageView>
        <ImageView fx:id="muleAnim" fitHeight="100.0" fitWidth="112.0" layoutX="461.0" layoutY="76.0" visible="false">
            <image>
                <Image url="@Resources/mule-anim.gif"/>
            </image>
        </ImageView>
        <ImageView fx:id="energyAnim" fitHeight="54.0" fitWidth="63.0" layoutX="72.0" layoutY="181.0" visible="false">
            <image>
                <Image url="@Resources/energy-anim.gif"/>
            </image>
        </ImageView>
    </children>
</AnchorPane>
